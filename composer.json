{
    "name": "mvdnbrk/dhlparcel-php-api",
    "description": "DHL Parcel API client for PHP",
    "keywords": [
        "dhl",
        "parcel",
        "api"
    ],
    "homepage": "https://github.com/mvdnbrk/dhlparcel-php-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "composer/ca-bundle": "^1.2",
        "guzzlehttp/guzzle": "^7.1",
        "lcobucci/jwt": "^3.4 || ^4.0",
        "ramsey/uuid": "^4.0",
        "tightenco/collect": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "symfony/var-dumper": "^5.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Mvdnbrk\\DhlParcel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdnbrk\\DhlParcel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
